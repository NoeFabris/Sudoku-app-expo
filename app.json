{
  "expo": {
    "name": "Sudoku",
    "slug": "sudoku",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dopesalmon.sudoku",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static-pwa",
      "favicon": "./assets/images/favicon.png",
      "name": "Sudoku",
      "shortName": "Sudoku",
      "startUrl": "/",
      "display": "standalone",
      "backgroundColor": "#FFFFFF",
      "themeColor": "#6a1b9a",
      "description": "Play Sudoku anywhere",
      "icons": [
        {
          "src": "./assets/images/pwa/icon-192.png",
          "sizes": "192x192",
          "type": "image/png"
        },
        {
          "src": "./assets/images/pwa/icon-512.png",
          "sizes": "512x512",
          "type": "image/png"
        },
        {
          "src": "./assets/images/pwa/icon-512-maskable.png",
          "sizes": "512x512",
          "type": "image/png",
          "purpose": "maskable"
        }
      ],
      "splash": {
        "image": "./assets/images/pwa/splash.png",
        "backgroundColor": "#FFFFFF",
        "resizeMode": "contain"
      }
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "59155d97-dbc2-44a0-b711-cef7ed051622"
      }
    },
    "owner": "dopesalmon"
  }
}
